main
    let int number
    output "Enter the number of week: "
    inputto number
    switch number
        case 1
            output "Sunday"
            break
        case 2
            output "Monday"
            break
        case 3
            output "Tuesday"
            break
        case 4
            output "Wednesday"
            break
        case 5
            output "Thursday"
            break
        case 6
            output "Friday"
            break
        case 7
            output "Saturday"
            break
        case default
            output "Unknown"
    end
end
